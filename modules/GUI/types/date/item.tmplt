<div class="form__inp-contain" role="input-contain">
  <label class="form__placeholder">
    <input type="date" id="{ field['alias'] }" class="form__inp form__inp--date" value="" role="date" />
    <span class="form__inp-empty" role="date-empty"></span>
  </label>
  <label class="form__placeholder">
    <input type="time" id="{ field['alias'] }-time" class="form__inp form__inp--time" value="" role="time" />
    <span class="form__inp-empty" role="time-empty"></span>
  </label>
</div>
