<component>
  <div class="form__inp-contain" data-role="input-contain">
    <label class="form__select">
      <select name={$alias} id={$alias} data-role="select">
        <if test={$settings.defaultValue == -1}>
          <option value="-1"></option>
        </if>
        <for-each key={$i} item={$data-item} from={$settings.defaultData}>
          <option value={$i} selected={$data == $i}>{$data-item}</option>
        </for-each>
      </select>
    </label>
  </div>
</component>
