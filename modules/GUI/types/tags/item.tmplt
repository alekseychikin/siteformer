{ import(Suggest, './components/form-suggests/form-suggests') }

<div class="form__inp-contain" data-role="input-contain">
  { rows = [] }
  { if (searchResult?) }
    { rows = searchResult }
  { endif }
  <Suggest rows={rows}>
    <input
      type="text"
      name="{ field.alias }"
      id="{ field.alias }"
      class="form__inp"
      value="{ data }"
      data-role="tags"
      autocomplete="off"
    />
  </Suggest>
</div>
