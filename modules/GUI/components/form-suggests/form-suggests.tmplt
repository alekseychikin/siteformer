<div class="form-suggests">
  { childs }
  { issetResults = rows? && arr_len(rows) }
  { if (issetResults) }
    <div class="form-suggests__results">
      { for (row, rows) }
        <div
          class="form-suggests__item"
          data-role="suggest-item"
          data-id="{row.id}"
          data-title="{row.title}"
        >
          {row.title}
        </div>
      { endfor }
    </div>
  { endif }
</div>
