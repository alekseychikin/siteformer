{ import(SectionList, './sections/item/section-list') }

<div class="section__row">
  <a href="/cms/{section}/add/" class="form__btn">Добавить запись</a>

  <button class="form__btn form__btn--right" data-role="config-user-fields" type="button">Настроить поля</button>
</div>
<div class="section__row" role="section-list">
  <SectionList user_fields={user_fields} fields={fields} data={data} section={section} />
</div>
